{
  "name": "@cohor/backend",
  "version": "0.1.0",
  "scripts": {
    "dev": "serverless offline --noPrependStageInUrl --reloadHandler --stage local",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "prisma:build": "prisma format && prisma generate",
    "prisma:migration:apply": "npm run prisma:build && prisma migrate dev",
